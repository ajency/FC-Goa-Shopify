#back-to-top {
  position: fixed;
  bottom: 30px;
  right: 15px;
  width: 40px;
  border-radius:50px;
  height: 40px;
  text-align: center;
  line-height: 30px;
  background: #fff;
  cursor: pointer;
  text-decoration: none;
  transition: all 0.5s ease 0s;
  transform: translate(80px, 0px);
  z-index: 9999;
  box-shadow: 1px 2px 6px 4px rgba(53, 57, 68, 0.2);
  i {
    color: $color-text;
    display: inline-block;
    line-height: 40px;
  }
  &.show{
  	transform: translate(0px, 0px);
  }
  &:hover{
  	background-color: #000;
  }
}


