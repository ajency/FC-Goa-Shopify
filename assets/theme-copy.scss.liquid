.menu_moblie{
  .iconsec{
    border-bottom: 1px solid #FF6600;
    border-radius: 0;
    padding-bottom: 24px;
    margin-bottom: 15px;
  }
  .menuleft{
    width: 100%;
  }
  .logo{
    svg{
        max-width: 100%;
        height: auto;
    }
    @media (max-width: 475px){
      padding-left: 0;
    }
  }
  &.box_contentmenu{
    background: linear-gradient(-90deg,#600f47 -2.96%,#060e66 89.32%);
    .crossicon{
      position: absolute;
      top: -10px;
      right: 0;
      .menuleft{
        .active{
              .btn_icon{
                width: 0px;
                &::before{
                  top: 0;
                  transform: rotate(-45deg);
                  transition: 0.3s;
                }
                &::after{
                  top: 0;
                  transform: rotate(45deg);
                  transition: 0.3s;
                }
              }
          }
        .btn_icon{
          position: absolute;
          right: 32px;
          top: 14px;
          width: 22px;
          height: 22px;
          transform: rotate(45deg);
          &:before{
            position: absolute;
            left: 15px;
            content: ' ';
            height: 33px;
            width: 0;
            background-color: white;
          }
          &:after {
            position: absolute;
            left: 10px;
              top: 10px;
            content: ' ';
            height: 22px;
            width: 2px;
            background-color: white;
          }
        }
      }
    }
    .tab-content{
        height: auto;
        border-bottom: 1px solid #FF6600;
        border-radius: 0;
        padding-bottom: 20px;
        margin-bottom: 20px;
        .navbar-nav{
          .nav-item{
            .changeColor{
            color: #ff5b01;
            font-weight: 400;
          }
            .dropdown-arrow{
              width: 0px;
              height: 0px;
              border-left: solid 5px #ff5b01;
              border-bottom: solid 5px transparent;
              border-top: solid 5px transparent;
              position: absolute;
              top: 14px;
              left: -14px;
              &.rotate {
                -moz-transition: all .5s linear;
                -webkit-transition: all .5s linear;
                transition: all .5s linear;
            }
            &.rotate.down {
                -moz-transform:rotate(90deg);
                -webkit-transform:rotate(90deg);
                transform:rotate(90deg);
            }
          }
            ul{
              padding-top: 0;
              li{
              font-size: 16px;
                line-height: 20px;
                padding: 10px 0;
                a{
                  &:hover{
                    border-left: 1px solid #FF6600;
                    color: #FF6600 ;
                    text-decoration: none;
                    padding-left: 12px;
                        transition: ease-in-out 0.3s;
                  }
                }
              }
            }
          }
        }
    }
    .home_link{
      a{
        color: #FF6600;
        font-size: 16px;
        font-weight: 600;
      }
    }
  }
  .menuright{
    /*max-width: 100%;*/
    width: auto;
      display: flex;
      align-items: center;
      justify-content: flex-end;
      .accout_login{
        a{
          /*font-size: 22px;*/
          color: #FA693A;
          display: inline-block;
          text-decoration: none;
          position: relative;
          line-height: 2;
          img{
            max-width: 23px;
          }
        }
        .login_box {
          position: absolute;
          top: 60px;
          left: 50px;
          background: #fff;
          padding: 10px 30px;
          box-shadow: 1px 5px 8px 0.5px rgba(0,0,0,0.2);
          opacity: 0;
          visibility: hidden;
          transition: all 0.5s ease;
          text-align: left;
          &.active{
            opacity: 1;
            visibility: visible;
            transition: all 0.5s ease;
            top: 60px;
            left: 24px;
            z-index: 9999;
            @media (min-width: 999px) and (max-width: 1199px){
              left: 70px;
            }
            @media(max-width: 767px){
              left: -24px;
            }
            @media (max-width: 575px){
              left: -60px;
            }
          }
          ul{
            li{ 
              a{
                text-decoration: none;
                color: #909090;
                font-size: 13px;
                text-transform: uppercase;
              }
            }
          }
        }
      }
      a{
        width: auto;
        img{
          max-width: 23px;
        }
        span{
          &.count_pr_incart{
            position: absolute;
            top: 10px;
            width: 20px;
            height: 20px;
            line-height: 20px;
            text-align: center;
            border-radius: 50%;
            left: 38px;
            font-size: 12px;
            display: block;
            background-color: #f50505;
            font-family: Montserrat;
            background: linear-gradient(108.28deg, #fb910b -62.31%, #ff2801 89.32%);
            @media (max-width: 425px){
              left: 32px;
            }
          }
        }
      }
      .js_search{
        padding: 9px 15px 0;
        @media (max-width: 425px){
          padding: 9px 10px 0 15px;
        }
      }
      .js_minicart{
        padding: 0 15px;
        @media (max-width: 425px){
          padding: 0 15px 0 10px;
        }
      }
    @media (max-width: 475px){
      padding-right: 0;
    }
  }
}

.product-listing{
  .product-info{
    .product-title{
      height: 50px;
      line-height: 20px;
      a{
        font-weight: 500;
        font-size: 16px;
        color: #060E59;
        text-transform: capitalize;
        &:hover{
          text-decoration: none;
        }
      }
      @media (max-width: 767px){
        text-align: center;
      }
    }
    .product-price{
      ins{
        font-size: 14px;
        line-height: 17px;
        color: #060E59;
        text-decoration: none;
      }
      @media (max-width: 767px){
        text-align: center;
      }
    }
  }
  .product-extra-link{
    position: absolute;
    top: 10px;
    right: 30px;
    a{
      color: #422755;
      &:hover{
        text-decoration: none;
      }
      @media (max-width: 767px){
        font-size: 2rem;
      }
    }
  }

  .product-thumb{
    background: #F5F5F5;
    padding: 24px;
  }
  .quickview-link{
    display: none;
  }
}


